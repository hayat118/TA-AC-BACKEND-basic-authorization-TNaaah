<% include partials/header %>
<main>
  <div>
    <form action="/podcasts/<%= podcast.id%>/edit" method="POST">
      <input type="text" name="title" placeholder="title" value="<%= podcast.title%>">
      <input type="text" name="artist" placeholder="artist" value="<%= podcast.artist%>">
      <input type="text" name="song" placeholder="song" value="<%= podcast.song%>">
      <select name="podcastplan" id="podcastplan" value="<%= podcast.podcastplan%>">
           <option value="Free">Free</option>
            <option value="VIP">VIP</option>
            <option value="Premium">Premium</option>
      </select>
      <input type="submit" value="Update">
    </form>
  </div>
</main>
<% include partials/footer %>